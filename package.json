{
  "name": "root",
  "private": true,
  "repository": "https://github.com/stopyellingatme/formvana",
  "author": "Taylor King <tking@kingking.io>",
  "description": "Searching for Form Nirvana with Typescript and Svelte.",
  "scripts": {
    "try:vite": "cd example && pnpm run build:vite",
    "dev": "cd example && pnpm run dev",
    "gen:test": "node test.js",
    "gen:decorators": "tsc -p ./package",
    "package": "pnpm run gen:decorators && cd package && pnpm run build && cd .. && node output.js && pnpm run dev",
    "package:vite": "pnpm run gen:decorators && cd package && pnpm run build:vite && cd .. && node output.js && pnpm run dev:vite"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.9",
    "@tailwindcss/forms": "^0.3.2",
    "autoprefixer": "^10.2.4",
    "class-validator": "^0.13.1",
    "cssnano": "^4.1.10",
    "postcss": "^8.2.6",
    "postcss-import": "^14.0.0",
    "superstruct": "^0.15.2",
    "tailwindcss": "^2.1.4",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@wessberg/rollup-plugin-ts": "^1.3.14",
    "fs-extra": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "regexparam": "^1.3.0",
    "rollup": "^2.50.5",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-postcss": "^4.0.0",
    "sirv-cli": "^1.0.12",
    "svelte": "^3.38.2",
    "svelte-preprocess": "^4.7.3",
    "typescript": "^4.3.2",
    "tslib": "2.2.0",
    "vite": "^2.2.3"
  },
  "engines": {
    "node": ">=10",
    "pnpm": ">=3"
  }
}